<?xml version='1.0' encoding='utf-8'?>
<widget id="cordova-plugin-gps-only" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>GpsOnly</name>
    <description>
        Cordova GPS Only Plugin For Android
    </description>
    <author email="bendraaisma@gmail.com" href="">
        Bernard Arjan Draaisma
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <engine name="android" spec="~7.0.0" />
</widget>
